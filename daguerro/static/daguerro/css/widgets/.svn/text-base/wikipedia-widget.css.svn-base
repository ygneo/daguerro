/* Wikipedia widget styles */

.ui-wikipedia-widget { 
    float: right; 
    font-size: 70%; 
    color: grey; 
    margin: 0; 
    padding: 0;
}

.ui-wikipedia-widget p { 
    margin: 0;
    padding: 0;
}

.ui-wikipedia-result-found { 
   display: none;
}

.ui-wikipedia-current-link { 
   display: none;
}

.ui-wikipedia-widget a { color: blue; text-decoration: underline; display: inline; }